{
  "expo": {
    "name": "Aora",
    "scheme": "aora",
    "slug": "aora",
    "description":"Video posting app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-small.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yasaswini.aora"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a00c13cb-564b-4253-87f7-c55a8c08ad64"
      }
    }
  }
}
